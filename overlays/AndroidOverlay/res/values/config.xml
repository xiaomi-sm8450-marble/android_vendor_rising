<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="config_bodyFontFamily">google-sans-text</string>
    <string name="config_bodyFontFamilyMedium">google-sans-text-medium</string>
    <string name="config_clockFontFamily">google-sans-clock</string>
    <string name="config_headlineFontFamily">google-sans</string>
    <string name="config_headlineFontFamilyMedium">google-sans-medium</string>
    <string name="config_headlineFontFeatureSettings">ss03</string>
    <!-- The list of components which should be automatically disabled for all devices. -->

    <!-- Controls the navigation bar interaction mode:
         0: 3 button mode (back, home, overview buttons)
         1: 2 button mode (back, home buttons + swipe up for overview)
         2: gestures only for back, home and overview -->
    <integer name="config_navBarInteractionMode">2</integer>
    
    <bool name="config_supportSmartPixels">true</bool>
    <bool name="config_pocketModeSupported">false</bool>

     <bool name="config_appCompatUserAppAspectRatioSettingsIsEnabled">true</bool>
     <bool name="config_appCompatUserAppAspectRatioFullscreenIsEnabled">true</bool>

     <bool name="config_faceAuthDismissesKeyguard">false</bool>
    
    <string translatable="false" name="config_appsNotReportingCrashes">com.android.systemui,app.lawnchair</string>
    
    <!-- The amount to scale fullscreen snapshots for Overview and snapshot starting windows. -->
    <item name="config_highResTaskSnapshotScale" format="float" type="dimen">0.8</item>
    
    <integer name="config_maxBitmapSizePx">412</integer>
    <integer name="config_maxNotifBitmapSizePx">384</integer>
</resources>
